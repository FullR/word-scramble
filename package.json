{
  "name": "word-jumble",
  "version": "0.0.10",
  "scripts": {
    "postinstall": "cordova prepare",
    "clean": "mkdir -p builds && mkdir -p www && rm -rf builds/* && rm -rf www/*",
    "compile": "scripts/compile",
    "start": "scripts/dev-server",
    "start:android": "npm run compile && cordova run android",
    "build": "cross-env NODE_ENV=production npm run compile && scripts/build-desktop && scripts/build-web && scripts/build-android",
    "build:desktop": "npm run compile && scripts/build-desktop",
    "build:web": "npm run compile && scripts/build-web",
    "build:android": "npm run compile && scripts/build-android",
    "upload": "scripts/upload",
    "release": "npm run compile && npm run upload",
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src mocha --recursive --require babel-polyfill --compilers js:babel-core/register 'src/**/*.test.js'"
  },
  "dependencies": {
    "bembam": "0.0.1",
    "lodash": "^4.0.0",
    "putainde-localstorage": "^4.0.0",
    "react": "^0.14.0",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dnd-touch-backend": "^0.2.0",
    "react-dom": "^0.14.1",
    "redux": "^3.3.1",
    "rx": "^4.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.5.1",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.1",
    "chai": "^3.5.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.19.0",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.8",
    "file-loader": "^0.8.4",
    "fs-promise": "^0.3.1",
    "glob": "^5.0.14",
    "glob-promise": "^1.0.4",
    "jsdom": "^8.0.2",
    "json-loader": "^0.5.3",
    "loader-utils": "^0.2.11",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.0.0",
    "node-sass": "^3.3.3",
    "pace": "0.0.4",
    "postcss-loader": "^0.8.1",
    "progress": "^1.1.8",
    "react-addons-test-utils": "^0.14.7",
    "sass-loader": "^2.0.1",
    "sprity": "^1.0.8",
    "style-loader": "^0.12.4",
    "susy": "^2.2.6",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.12.1"
  }
}
