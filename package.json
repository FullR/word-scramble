    "lodash": "^4.0.0",
    "putainde-localstorage": "^4.0.0",
    "radium": "^0.16.2",
    "react": "^0.14.0",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dnd-touch-backend": "^0.2.0",
    "react-dom": "^0.14.1"
  },
  "devDependencies": {
    "audio-converter": "^1.0.7",
    "autoprefixer-loader": "^3.1.0",
    "babel-loader": "^5.3.2",
